#!/usr/bin/env python3

from fire import Fire
from seread import sng
from seread.version import __version__


def launch(MODE="help",p=1):
    if MODE=="help":
        print("h... available modes: help ...")
        return
    if MODE=="    ":print("ok")
    if MODE=="    ":print("ok")
    return p


if __name__=="__main__":
    print("D... seread script; version:",__version__)
    Fire( launch )
